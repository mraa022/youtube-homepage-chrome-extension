{

	"manifest_version": 2,
	"name": "static_youtube_home_page",
	"version":"0.1.0",
	"author":"Adnan Badri",
	"browser_action":{
		"default_popup":"popup.html"
	},
	"permissions": [
    	"<all_urls>"
  	],
	"background": {
   		"persistent": false,
   		"scripts": ["background.js"]

  	},
	"content_scripts":[
		{
			"matches":["https://*.youtube.com/*"],
			"js":["script.js"],
			"css":["style.css"]
		}
	]

}